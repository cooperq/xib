<h1>Listing user_projects</h1>

<table>
  <tr>
    <th>User</th>
    <th>Project</th>
  </tr>

<% for user_projects in @user_projects %>
  <tr>
    <td><%=h user_projects.user_id %></td>
    <td><%=h user_projects.project_id %></td>
    <td><%= link_to 'Show', user_projects %></td>
    <td><%= link_to 'Edit', edit_user_projects_path(user_projects) %></td>
    <td><%= link_to 'Destroy', user_projects, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user_projects', new_user_projects_path %>
